<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape>
            <solid
                android:color="@color/colorButtonSecondary" />
            <stroke
                android:width="3dp"
                android:color="@color/colorButtonPrimaryDark" />
            <corners
                android:radius="25dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/colorButtonSecondary"/>
            <stroke
                android:width="1dp"
                android:color="@color/colorButtonPrimaryDark" />
            <corners
                android:radius="25dp" />
        </shape>
    </item>
</selector>